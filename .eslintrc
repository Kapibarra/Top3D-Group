{
    "globals": {
        "$": true
      },
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser":true,
        "node":true,
        "jquery": true
    },
    "rules": {
"semi":"off",
"arrow-parents": "off",
"comma-dengle": "off",
"linebreak-style" : "off",
"object-curly-spacing": "off",
"quotes":"off",
"indent":"off",
"require-jsdoc":"off",
"eol-last":"off",
"operator-linebreak": "off",
"no-trailing-spaces": "off",
"func-call-spacing" : "off",
"max-len": "off",
"no-invalid-this": 0


    },
    "extends": ["eslint:recommended", "google"]
}
